{
  "name": "hummersmyth",
  "version": "1.1.1",
  "description": "Hummersmyth is a tiny and simple yet powerful HTML renderer and DOM creator for Node.js and the browser.",
  "main": "hummersmyth.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "grunt build",
    "coverage": "nyc ./node_modules/.bin/jasmine",
    "lint": "grunt jshint",
    "test": "./node_modules/.bin/jasmine && npx karma start test/karma/karma.conf.js --single-run",
    "test-node": "./node_modules/.bin/jasmine",
    "test-browser": "npx karma start test/karma/karma.conf.js --single-run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tbillenstein/hummersmyth.git"
  },
  "bugs": {
    "url": "https://github.com/tbillenstein/hummersmyth/issues"
  },
  "homepage": "https://github.com/tbillenstein/hummersmyth#readme",
  "keywords": [
    "html",
    "tags",
    "elements",
    "render",
    "renderer",
    "dom",
    "builder",
    "creator",
    "snippet",
    "injection",
    "template",
    "engine"
  ],
  "author": "tbillenstein <tb@thomasbillenstein.com> (https://thomasbillenstein.com)",
  "license": "MIT",
  "nyc": {
    "exclude": [
      "**/*Spec.js"
    ],
    "reporter": [
      "text",
      "html"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "grunt": "^1.6.1",
    "grunt-contrib-jshint": "^3.2.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-terser": "^2.0.0",
    "jasmine": "^5.13.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-firefox-launcher": "^2.1.3",
    "karma-jasmine": "^5.1.0",
    "nyc": "^17.1.0"
  }
}
